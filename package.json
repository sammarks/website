{
  "name": "gatsby-starter-typescript-plus",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "Resi Respati <resir014@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -d public -r https://$GITHUB_TOKEN@github.com/sammarks/website.git -m \"chore: publish github pages [ci skip]\"",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "gatsby serve --port 5020",
    "test": "npm run type-check && npm run lint",
    "test:integration": "start-server-and-test dev http://localhost:8000 cypress:run",
    "type-check": "tsc",
    "generate": "graphql-codegen",
    "cypress": "is-ci \"test:integration\" \"cypress:open\"",
    "cypress:run": "CYPRESS_BASE_URL=http://localhost:8000 cypress run --record --key 2210fbe2-0f3a-4e37-8260-1a509abde339",
    "cypress:open": "cypress open",
    "lighthouse": "lhci autorun"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@babel/helper-validator-identifier": "^7.9.5",
    "@thesisedu/web": "^0.49.0",
    "@testing-library/cypress": "^6.0.0",
    "@types/pluralize": "^0.0.29",
    "@types/rehype-react": "^4.0.0",
    "antd": "^4.0.2",
    "axios": "^0.19.2",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "fs-extra": "^9.0.0",
    "gatsby": "^2.19.43",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-canonical-urls": "^2.1.21",
    "gatsby-plugin-catch-links": "^2.3.0",
    "gatsby-plugin-google-analytics": "^2.3.0",
    "gatsby-plugin-import": "^2.1.5",
    "gatsby-plugin-less": "^3.0.20",
    "gatsby-plugin-manifest": "^2.2.47",
    "gatsby-plugin-offline": "^3.0.40",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sharp": "^2.4.12",
    "gatsby-plugin-styled-components": "^3.1.20",
    "gatsby-plugin-typescript": "^2.2.3",
    "gatsby-remark-copy-linked-files": "^2.1.39",
    "gatsby-remark-custom-blocks": "^2.3.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.49",
    "gatsby-remark-prismjs": "^3.3.35",
    "gatsby-remark-responsive-iframe": "^2.2.33",
    "gatsby-remark-smartypants": "^2.1.22",
    "gatsby-source-filesystem": "^2.1.55",
    "gatsby-transformer-gitinfo": "^1.1.0",
    "gatsby-transformer-json": "^2.2.27",
    "gatsby-transformer-remark": "^2.6.58",
    "gatsby-transformer-sharp": "^2.3.18",
    "less": "^3.11.1",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "object-reference": "^0.0.4",
    "pluralize": "^8.0.0",
    "polished": "^3.4.4",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "puppeteer": "^3.0.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-share": "^4.1.0",
    "rehype-react": "^5.0.1",
    "styled-components": "^5.0.1",
    "typescript": "^3.8.3"
  },
