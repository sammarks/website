{
  "name": "gatsby-starter-typescript-plus",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "Resi Respati <resir014@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "generate": "graphql-codegen"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "antd": "^4.0.2",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "gatsby": "^2.19.43",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-canonical-urls": "^2.1.21",
    "gatsby-plugin-import": "^2.1.5",
    "gatsby-plugin-less": "^3.0.20",
    "gatsby-plugin-manifest": "^2.2.47",
    "gatsby-plugin-offline": "^3.0.40",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sharp": "^2.4.12",
    "gatsby-plugin-styled-components": "^3.1.20",
    "gatsby-plugin-typescript": "^2.2.3",
    "gatsby-remark-copy-linked-files": "^2.1.39",
    "gatsby-remark-images": "^3.1.49",
    "gatsby-remark-prismjs": "^3.3.35",
    "gatsby-remark-responsive-iframe": "^2.2.33",
    "gatsby-remark-smartypants": "^2.1.22",
    "gatsby-source-filesystem": "^2.1.55",
    "gatsby-transformer-json": "^2.2.27",
    "gatsby-transformer-remark": "^2.6.58",
    "gatsby-transformer-sharp": "^2.3.18",
    "less": "^3.11.1",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "object-reference": "^0.0.4",
    "polished": "^3.4.4",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.0",
    "@graphql-codegen/typescript": "^1.13.0",
    "@graphql-codegen/typescript-operations": "^1.13.0",
    "@types/classnames": "^2.2.10",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^6.8.0",
    "eslint-config-blvd": "^1.0.2",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  }
}
